translate spanish tatsuki28_goodbye_b034c64f:

    # sg "「You should probably have tried confronting him. 」"
    sg "「Quizás una mejor idea sería confrontarlo. 」"

# game/Day 28.rpy:191
translate spanish tatsuki28_goodbye_826e4b27:

    # sg "「When your friend is feeling down,\n \ sometimes kind words fail,\n \ and so you need to be harsh. 」"
    sg "「Cuando alguien que queremos se siente mal,\n \ a veces ser dulce no es efectivo,\n \ y es necesario ponernos firmes. 」"

# game/Day 28.rpy:195
translate spanish tatsuki28_goodbye_74831180:

    # cu "「I see! As expected!\n \ Then next time should go okay! 」"
    cu "「¡Ya entiendo! ¡como se esperaba!\n \ ¡la próxima vez seguro funcionará! 」"

# game/Day 28.rpy:196
translate spanish tatsuki28_goodbye_572cbf28:

