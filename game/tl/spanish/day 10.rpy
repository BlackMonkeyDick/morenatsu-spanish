translate spanish tatsuki10_eee1baa9:

    # ta "「Gyaah!! 」"
    ta "「¡¡Aaah!! 」"

# game/day 10.rpy:1664
translate spanish tatsuki10_0778bd09:

    # ta "「Hmm... That was gross.{p} \ {nw}"
    ta "「Mmm... eso no fue impulsivo.{p} \ {nw}"

# game/day 10.rpy:1666
translate spanish tatsuki10_3765a4d7:

    # extend "You're disgusting, [fn]. 」"
    extend "Eres desagradable, [fn]. 」"

