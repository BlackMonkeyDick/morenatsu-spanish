translate spanish tatsuki23_cf92f84c:

    # ta "「I said I'm okay. You worry too much. 」"
    ta "「Dije que me siento bien. Te preocupas demasiado. 」"

# game/Day 23.rpy:296
translate spanish tatsuki23_d7528722:

    # cu "「B-{w=.2}but... 」"
    cu "「P-{w=.2}pero... 」"

