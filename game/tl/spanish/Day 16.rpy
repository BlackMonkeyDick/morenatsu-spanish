translate spanish shin16_garden_c7319d50:

    # fn "「S-Shin-kun? 」"
    fn "「¿S-Shin-kun? 」"

# game/Day 16.rpy:3660
translate spanish shin16_garden_583fa551:

    # si "「... 」"
    si "「... 」"

# game/Day 16.rpy:3661
