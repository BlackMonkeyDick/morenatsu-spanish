    extend "{cps=15}Aventura 3 perdida"

# game/day 9.rpy:727
translate spanish shun09_oldgame_7b3eeaf9:

    # "{cps=15}Sorry."
    "{cps=15}Lo lamentamos."

# game/day 9.rpy:731
translate spanish shun09_oldgame_250e0ffb:

    # su "「... 」"
    su "「... 」"

# game/day 9.rpy:732
