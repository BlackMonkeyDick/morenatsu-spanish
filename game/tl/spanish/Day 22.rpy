# game/Day 22.rpy:6141
translate spanish juuichi22_d3d051c4:

    # ju "「Listen, {w=.2}[ln].{w} {nw}"
    ju "「Escucha, {w=.2}[ln].{w} {nw}"

# game/Day 22.rpy:6143
translate spanish juuichi22_98ae9d14:

