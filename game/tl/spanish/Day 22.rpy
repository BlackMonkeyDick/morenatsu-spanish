translate spanish juuichi22_9a296daa:

    # fn "「I am in love with you. 」"
    fn "「Estoy enamorado de ti. 」"

# game/Day 22.rpy:6121
translate spanish juuichi22_839088a7:

    # "I changed it up a bit.{p}His eyes widen at my straightforward confession."
    "Lo cambié un poco.{p}Sus ojos se abren enormemente ante mi declaración tan directa."

# game/Day 22.rpy:6125
translate spanish juuichi22_3ec6dbbd:

    # ju "「W-{w=.2}wait...{w} are you being serious? 」"
    ju "「A-{w=.2}alto...{w} ¿me lo dices en serio? 」"

# game/Day 22.rpy:6126
translate spanish juuichi22_c96f039c:

    # fn "「I'm not joking! 」"
    fn "「¡No estoy bromeando! 」"

# game/Day 22.rpy:6130
translate spanish juuichi22_6031c79d:

