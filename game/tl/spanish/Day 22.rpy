# game/Day 22.rpy:6149
translate spanish juuichi22_02f8908a:

    # fn "「Juuichi-san, {w=.2}I, {w=.2}I... 」"
    fn "「Juuichi-san, {w=.2}yo, {w=.2}yo... 」"

# game/Day 22.rpy:6150
